{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/index.ts", "bin/bunpress.js"],
  "project": ["src/**/*.ts", "!src/**/__tests__/**"],
  "ignoreDependencies": [
    "react",
    "react-dom",
    "happy-dom",
    "ws"
  ],
  "ignoreBinaries": [],
  "ignoreExportsUsedInFile": true,
  "paths": {
    "@/*": ["./themes/docs/*"]
  },
  "rules": {
    "dependencies": "error",
    "devDependencies": "warn",
    "exports": "warn"
  }
} 